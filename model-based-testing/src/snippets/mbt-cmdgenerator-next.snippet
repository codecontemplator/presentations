    <Snippet>
      <Code Language="csharp" Delimiter="$"><![CDATA[            return
                from nationalIdRaw in Gen.ArrayOf(10, Gen.Choose(0,9))
                from country in Gen.Elements("Sweden", "Denmark", "Norway")
                from eyeColor in Gen.Elements("Blue", "Green", "Brown")
                from name in Arb.Generate<string>()
                let nationalId = DigitsToString(nationalIdRaw)
                let entity = new PersonEntity { RowKey = nationalId, PartitionKey = country, EyeColor = eyeColor, Name = name }
                select new InsertOrReplaceCommand(entity) as Command<PersonStore, PersonStoreSimulator>;
]]></Code>
    </Snippet>
  </CodeSnippet>
