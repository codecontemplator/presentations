    <Snippet>
      <Code Language="csharp" Delimiter="$"><![CDATA[        private static bool SearchResultsAreEquivalent(IEnumerable<PersonEntity> a, IEnumerable<PersonEntity> b)
        {
            return Enumerable.SequenceEqual(
                a.OrderBy(x => x.RowKey), 
                b.OrderBy(x => x.RowKey), 
                PersonEntityComparer.Instance);
        }
]]></Code>
